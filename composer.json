{
  "name": "asmaster/equip-twig",
  "description": "The Twig integration for the Equip framework",
  "type": "library",
  "keywords": [
    "equip",
    "twig",
    "formatter",
    "renderer"
  ],
  "license": "MIT",
  "homepage": "https://github.com/AlexMasterov/equip-twig",
  "authors": [{
    "name": "Alex Masterov",
    "email": "alex.masterow@gmail.com"
  }],
  "require": {
    "php": "^5.5.9 || ^7.0",
    "equip/framework": "^1.0",
    "josegonzalez/dotenv": "^1.0 || ^2.0",
    "twig/twig": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.5 || ^5.0.5"
  },
  "autoload": {
    "psr-4": {
      "Asmaster\\EquipTwig\\": "src/"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Asmaster\\EquipTwig\\Tests\\": "tests/"
    }
  }
}
