{
  "name": "asmaster/equip-twig",
  "description": "The Twig integration for the Equip framework",
  "type": "library",
  "keywords": [
    "equip",
    "twig",
    "formatter",
    "renderer"
  ],
  "license": "MIT",
  "homepage": "https://github.com/AlexMasterov/equip-twig",
  "authors": [{
    "name": "Alex Masterov",
    "email": "alex.masterow@gmail.com"
  }],
  "require": {
    "php": "^5.5.9 || ^7.0",
    "equip/framework": "^1.0",
    "twig/twig": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "Asmaster\\EquipTwig\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Asmaster\\EquipTwig\\Tests\\": "tests/"
    }
  },
  "suggest": {
    "asmaster/twig-extension": "For a custom extensions",
    "josegonzalez/dotenv": "For environment based configuration loading"
  }
}
